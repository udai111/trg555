{
  "name": "quantum-trader-pro",
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced trading platform with AI/ML capabilities",
  "scripts": {
    "dev:server": "cross-env NODE_OPTIONS=\"--trace-warnings\" PORT=5051 tsx watch server/index.ts",
    "dev:client": "cd client && cross-env PORT=3000 vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && tsc && vite build",
    "lint": "cd client && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "cd client && vite preview"
  },
